{% embed '@atoms/grid/_grid-1-up.twig' with { container: true } %}
  {% block column_1 %}
    <h1>Branding Examples</h1>
    <p class="lead">Different applications of a branding block.</p>

    {# Provide the things here #}

    {% embed '@atoms/grid/_grid-3-up.twig' with { container: false } %}
      {% block column_1 %}
        {% include "@molecules/_branding.twig" with {
          url: '#',
          logo_inline_svg: '@atoms/image/logo.svg',
          site_name: 'blah',
          site_slogan: 'more blah'
        } %}
      {% endblock column_1 %}
      {% block column_2 %}
        {% include "@molecules/_branding.twig" with {
          url: '#',
          logo_img_url: paths.assets ~ '/logo.svg',
          site_name: 'blah',
          site_slogan: 'more blah'
        } %}

      {% endblock column_2 %}
    {% endembed %}
  {% endblock column_1 %}
{% endembed %}

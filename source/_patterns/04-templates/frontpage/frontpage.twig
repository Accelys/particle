{% embed '@atoms/alert/_alert.twig' with alert_info %}
{% endembed %}
<header role="banner">
  {% embed '@molecules/jumbotron/_jumbotron.twig' with { jumbotron_fluid: true } %}
    {% block jumbotron_branding %}
      {% include "@atoms/_branding.twig" with {
        url: '#',
        logo_svg_inline: '@atoms/image/logo.svg',
        site_name: 'Phase2 Particle',
        site_slogan: 'This is a dev environment to work on the latest version of the Particle theme. Make good things happen!'
      } %}
    {% endblock jumbotron_branding %}
  {% endembed %}
</header>

{% embed '@atoms/grid/_grid-1-up.twig' with { container: true } %}
  {% block column_1 %}

    {% include '@organisms/card-grid/_card-grid.twig' %}

  {% endblock %}
{% endembed %}

<footer class="container p-3 footer" role="contentinfo">
  <hr>
  {{ footer_content.slogan }}
</footer>
